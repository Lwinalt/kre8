{
  "name": "kre8",
  "version": "1.0.0",
  "description": "Kre8 electron app",
  "main": "./src/main/main.js",
  "scripts": {
    "test": "NODE_ENV=test mocha",
    "build": "webpack --config webpack.config.js",
    "build:test": "webpack --config webpack.prod.config.js",
    "build:prod": "webpack --config webpack.prod.config.js",
    "prod": "webpack --mode production --config webpack.prod.config.js && NODE_ENV=production electron --noDevServer src/main/main.js",
    "prestart": "npm run build",
    "postinstall": "electron-builder install-app-deps",
    "start": "electron src/main/main.js",
    "dist": "DEBUG=electron-builder electron-builder",
    "dev": "NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --port 3000 --config=./webpack.config.js"
  },
  "build": {
    "appId": "com.kre8.app",
    "productName": "Kre8",
    "mac": {
      "target": "dmg",
      "category": "public.app-category.developer-tools"
    },
    "directories": {
      "buildResources": "src/client/styles/assets",
      "output": "build"
    },
    "files": [
      "**/*",
      "./dist/**/*",
      "./src/main/main.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bradenaa/kre8.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bradenaa/kre8/issues"
  },
  "homepage": "https://github.com/bradenaa/kre8#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-preset-airbnb": "^3.1.0",
    "chai": "^4.2.0",
    "css-loader": "^2.1.0",
    "electron": "^4.0.4",
    "electron-builder": "^20.39.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "fsevents": "^1.2.7",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^6.0.2",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@vx/gradient": "0.0.183",
    "@vx/group": "0.0.183",
    "@vx/hierarchy": "0.0.183",
    "@vx/shape": "0.0.184",
    "aws-sdk": "^2.402.0",
    "d3-hierarchy": "^1.1.8",
    "d3-shape": "^1.3.4",
    "dotenv": "^6.2.0",
    "i": "^0.3.6",
    "mkdirp": "^0.5.1",
    "npm": "^6.8.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "simple-react-validator": "^1.0.5",
    "uuid": "^3.3.2",
    "yamljs": "^0.3.0",
    "yup": "^0.27.0"
  }
}
